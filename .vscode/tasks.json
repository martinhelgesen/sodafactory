{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/testsolution.sln"
            ],
            "problemMatcher": "$msCompile",
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/sodafactoryclient/sodafactoryclient.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/sodafactoryclient/sodafactoryclient.csproj"
            ],
            "problemMatcher": "$msCompile",
            "presentation": {
                "focus": true
            }
        }
    ]
}